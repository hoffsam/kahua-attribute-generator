{
  "name": "kahua-attribute-generator",
  "displayName": "Kahua XML Generator",
  "description": "Generate XML for Kahua AppDefs : attributes, labels, DataTags, Data Store Columns, FieldDefs, etc from selected lines of comma seperated values.",
  "version": "3.2.1",
  "publisher": "Sammy",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run esbuild-prod",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild-prod": "npm run esbuild-base -- --minify --sourcemap=inline",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap=inline --watch",
    "compile": "tsc -p . --noEmit",
    "watch": "tsc -watch -p . --noEmit",
    "package": "vsce package",
    "build": "npm run esbuild-prod && npm run package",
    "pretest": "npm run compile-tests",
    "compile-tests": "tsc -p . --outDir out --module commonjs",
    "test": "npm run compile-tests && npx mocha out/test/suite/simple.test.js",
    "test:unit": "npm run compile-tests && npx mocha --ui tdd out/test/suite/{simple,unit,conditionals}.test.js --grep \"Unit Tests|Simple Unit Tests\"",
    "test:integration": "npm run compile-tests && node ./out/test/runTest.js",
    "test:xpath": "npx mocha src/test/xpath-template.standalone.test.js",
    "test:formatting": "npx mocha src/test/injection-formatting.standalone.test.js", 
    "test:standalone": "npm run test:xpath && npm run test:formatting",
    "test:all": "npm run test:standalone && npm run test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.122",
    "@types/vscode": "^1.85.0",
    "@types/sinon": "^17.0.3",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.0",
    "glob": "^11.0.3",
    "mocha": "^10.8.2",
    "sinon": "^18.0.0",
    "typescript": "^5.4.0"
  },
  "activationEvents": [
    "onLanguage:xml"
  ],
  "contributes": {
    "languages": [
      {
        "id": "xml",
        "aliases": [
          "XML"
        ],
        "extensions": [
          ".xml"
        ]
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "kahua.submenu",
          "when": "config.kahua.showInContextMenu && kahua.hasApplicableDocument",
          "group": "navigation"
        }
      ],
      "kahua.submenu": [
        {
          "command": "kahua.showTemplateForGeneration",
          "group": "kahua@1",
          "when": "config.kahua.showTemplatesInMenu && kahua.hasApplicableDocument && kahua.canGenerateTemplates"
        },
        {
          "command": "kahua.showSnippetForGeneration",
          "group": "kahua@2", 
          "when": "config.kahua.showSnippetsInMenu && kahua.hasApplicableDocument && kahua.canGenerateSnippets"
        },
        {
          "command": "kahua.generateIntoNewEditor",
          "group": "kahua@3",
          "when": "kahua.isTemplateDocument || kahua.isSnippetDocument"
        },
        {
          "command": "kahua.generateAtCursor",
          "group": "kahua@4",
          "when": "kahua.isTemplateDocument || kahua.isSnippetDocument"
        },
        {
          "command": "kahua.injectIntoSourceFile",
          "group": "kahua@5",
          "when": "(kahua.isTemplateDocument || kahua.isSnippetDocument) && kahua.hasSourceFile"
        },
        {
          "command": "kahua.injectIntoFile",
          "group": "kahua@6",
          "when": "kahua.isTemplateDocument || kahua.isSnippetDocument"
        }
      ]
    },
    "submenus": [
      {
        "id": "kahua.submenu",
        "label": "Kahua"
      }
    ],
    "commands": [
      {
        "command": "kahua.showTemplateForGeneration",
        "title": "Kahua: Show Template for Generation"
      },
      {
        "command": "kahua.showSnippetForGeneration",
        "title": "Kahua: Show Snippet for Generation"
      },
      {
        "command": "kahua.generateIntoNewEditor",
        "title": "Kahua: Generate into New Editor"
      },
      {
        "command": "kahua.generateAtCursor",
        "title": "Kahua: Generate at Cursor"
      },
      {
        "command": "kahua.injectIntoSourceFile",
        "title": "Kahua: Inject into Source File"
      },
      {
        "command": "kahua.injectIntoFile",
        "title": "Kahua: Inject into File"
      }
    ],
    "keybindings": [],
    "configuration": {
      "title": "Kahua Attribute Generator",
      "properties": {
        "kahua.showInContextMenu": {
          "type": "boolean",
          "default": true,
          "description": "Show Kahua generator commands in the editor right-click context menu."
        },
        "kahua.outputTarget": {
          "type": "string",
          "enum": [
            "clipboard",
            "newEditor"
          ],
          "enumDescriptions": [
            "Copy generated XML to clipboard",
            "Open generated XML in a new editor window"
          ],
          "default": "newEditor",
          "description": "Choose where to output the generated XML."
        },
        "kahua.suppressInvalidConditionWarnings": {
          "type": "boolean",
          "default": false,
          "scope": "resource",
          "description": "Suppress error notifications when conditional expressions reference tokens that don't exist or have no value."
        },
        "kahua.showSnippetsInMenu": {
          "type": "boolean",
          "default": true,
          "description": "Show snippet generation commands in the editor right-click context menu. Only applies when kahua.showInContextMenu is enabled."
        },
        "kahua.showTemplatesInMenu": {
          "type": "boolean",
          "default": true,
          "description": "Show template generation commands in the editor right-click context menu. Only applies when kahua.showInContextMenu is enabled."
        },
        "kahua.autoDetectDocumentType": {
          "type": "boolean",
          "default": true,
          "description": "Automatically detect XML document type on open and update the simplified workflow context."
        },
        "kahua.defaultSnippetTableRows": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "maximum": 100,
          "description": "Default number of table rows to generate in snippets. Set to 0 to disable and use current behavior (single row). When enabled, will prompt for row count if not specified."
        },
        "kahua.documentTypes": {
          "type": "array",
          "default": [
            {
              "id": "extension",
              "name": "Extension XML",
              "priority": 200,
              "rules": [
                {
                  "kind": "rootElement",
                  "value": "App"
                },
                {
                  "kind": "xpathNotExists",
                  "xpath": "App/App.Supplements"
                }
              ]
            },
            {
              "id": "supplement",
              "name": "Supplement XML",
              "priority": 100,
              "rules": [
                {
                  "kind": "rootElement",
                  "value": "App"
                },
                {
                  "kind": "xpathExists",
                  "xpath": "App/App.Supplements"
                }
              ]
            }
          ],
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier for this document type."
              },
              "name": {
                "type": "string",
                "description": "Display name shown in quick picks or logs."
              },
              "priority": {
                "type": "number",
                "description": "Higher numbers run first. The first matching type wins."
              },
              "rules": {
                "type": "array",
                "description": "All rules must match for the document type to apply.",
                "items": {
                  "type": "object",
                  "properties": {
                    "kind": {
                      "type": "string",
                      "enum": [
                        "rootElement",
                        "xpathExists",
                        "xpathNotExists"
                      ],
                      "description": "Rule logic: match root tag or check if an XPath-like path exists/does not exist."
                    },
                    "value": {
                      "type": "string",
                      "description": "Required when kind is 'rootElement'. The tag name to match."
                    },
                    "xpath": {
                      "type": "string",
                      "description": "Required when kind is 'xpathExists' or 'xpathNotExists'. Slash-delimited path such as 'App/App.Supplements'."
                    }
                  },
                  "required": [
                    "kind"
                  ]
                }
              }
            },
            "required": [
              "id",
              "name",
              "rules"
            ]
          },
          "description": "Defines XML document types that Kahua understands. Detection is driven by ordered rules and the first matching type wins."
        },
        "kahua.tokenNameDefinitions": {
          "type": "array",
          "scope": "resource",
          "default": [
            {
              "id": "appname",
              "name": "App Name Header",
              "type": "header",
              "tokens": "appname,entity:Field",
              "tokenReadPaths": {
                "appname": {
                  "type": "attribute",
                  "path": "App/@Name"
                },
                "entity": {
                  "type": "selection",
                  "path": "EntityDefs/EntityDef",
                  "attribute": "Name",
                  "affectsInjection": true,
                  "injectionPathTemplate": "App/EntityDefs/EntityDef[@Name='{value}']/Attributes"
                }
              }
            },
            {
              "id": "attributes",
              "name": "Attribute Tokens",
              "type": "table",
              "tokens": "name,type:Text,visualtype:TextBox,label,descriptionlabel,linkedEntityDef"
            },
            {
              "id": "lookupheader",
              "name": "Lookup Header Tokens",
              "type": "header",
              "tokens": "entity,listname"
            },
            {
              "id": "lookups",
              "name": "Lookup Tokens",
              "type": "table",
              "tokens": "value,label"
            }
          ],
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier for this token name definition"
              },
              "name": {
                "type": "string",
                "description": "Display name for this token name definition"
              },
              "type": {
                "type": "string",
                "enum": [
                  "header",
                  "table"
                ],
                "description": "Type of token processing: 'header' for single-line parsing, 'table' for row-by-row parsing"
              },
              "tokens": {
                "type": "string",
                "description": "Comma-separated list of token names. Format: 'name,type' or 'name:DefaultValue,type:DefaultValue'"
              },
              "tokenReadPaths": {
                "type": "object",
                "description": "Optional configuration for reading token values from source XML files",
                "additionalProperties": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "attribute",
                        "text",
                        "selection"
                      ],
                      "description": "Type of read: 'attribute' reads an XML attribute value, 'text' reads element text content, 'selection' prompts user to select from multiple options"
                    },
                    "path": {
                      "type": "string",
                      "description": "XPath-like expression to locate the value. For attributes use '@AttributeName' at the end (e.g., 'App/@Name'). For selection type, path to elements to choose from"
                    },
                    "attribute": {
                      "type": "string",
                      "description": "For selection type: which attribute to use for display and value (e.g., 'Name')"
                    },
                    "affectsInjection": {
                      "type": "boolean",
                      "description": "If true, the selected value will be used to modify injection paths dynamically (e.g., entity selection determines which EntityDef to inject into)"
                    },
                    "injectionPathTemplate": {
                      "type": "string",
                      "description": "Template for dynamic injection path. Use {value} as placeholder for the selected token value (e.g., 'EntityDefs/EntityDef[@Name='{value}']/Attributes')"
                    }
                  },
                  "required": [
                    "type",
                    "path"
                  ]
                }
              }
            },
            "required": [
              "id",
              "name",
              "type",
              "tokens"
            ]
          },
          "description": "Definitions of token name sets that can be referenced by fragments"
        },
        "kahua.fragmentDefinitions": {
          "type": "array",
          "scope": "resource",
          "default": [
            {
              "id": "attributes",
              "name": "Extension Attributes",
              "applicableDocumentTypes": [
                "extension"
              ],
              "tokenReferences": [
                "appname",
                "attributes"
              ],
              "injectionPaths": {
                "Attributes": "App/EntityDefs/EntityDef/Attributes",
                "Labels": "App/Cultures/Culture[@Code='en']/Labels",
                "LookupListLabels": "App/Cultures/Culture[@Code='en']/Labels",
                "DataTags": "App/DataTags",
                "LookupLists": "App/LookupLists",
                "LookupList": "App/LookupLists",
                "FieldDefs": "App/App.HubDefs/HubDef/HubDef.LogDef/LogDef.FieldDefs",
                "DataStore": "App/DataStore/Tables/Table/Columns",
                "ImportDefs": "App/App.HubDefs/HubDef/HubDef.ImportDefs/ImportDef/ImportDef.Sheets/Sheet/Sheet.Columns"
              },
              "fragments": {
                "body": {
                  "Attributes": "<Attribute Name=\"{$name}\" Label=\"[{$entity}_{$name}Label]\" Description=\"[{$entity}_{$name}Description]\" DataType=\"{$type}\"{'{$type}'=='Lookup' || '{$type}'=='LookupList' ? ' LookupListName=\"{$appname|raw}.{$entity}_{$name}LookupList\"' : ''}{'{$type}'=='Entity' || '{$type}'=='EntityDef' ? ' LinkedEntityDef=\"{$linkedEntityDef|raw}\"' : ''} IsConfigurable=\"true\" />",
                  "Labels": "<Label Key=\"{$entity}_{$name}Label\">{'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}</Label>\n<Label Key=\"{$entity}_{$name}Description\">{'{$descriptionlabel}'!='' ? '{$descriptionlabel|friendly}' : {'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}}</Label>",
                  "{'{$type}'=='Lookup' ? 'LookupListLabels' : ''}": "<Label Key=\"{$entity}_{$name}LookupListLabel\">{'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}</Label>\n<Label Key=\"{$entity}_{$name}LookupListDescription\">{'{$descriptionlabel}'!='' ? '{$descriptionlabel|friendly}' : {'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}}</Label>",
                  "DataTags": "<DataTag Name=\"{$entity}_{$name}\" Label=\"[{$entity}_{$name}Label]\" CultureLabelKey=\"{$entity}_{$name}Label\" />",
                  "Fields": "<Field Attribute=\"{$name}\" />",
                  "FieldDefs": "<FieldDef Name=\"{$name}\" Path=\"{$name}\" DataTag=\"{$entity}_{$name}\" />",
                  "DataStore": "<Column AttributeName=\"{$name}\" />",
                  "LogFields": "<Field FieldDef=\"{$name}\" />",
                  "ImportDefs": "<Column AttributeName=\"{$name}\" Name=\"{$name|friendly}\" />",
                  "VisualsTextBlock": "<TextBlock Name=\"{$name}\" DataTag=\"{$entity}_{$name}\" Path=\"{$name}\" />",
                  "VisualsPreview": "<{$visualtype} Name=\"{$name}\" DataTag=\"{$entity}_{$name}\" Path=\"{$name}\" IsReadOnly=\"True\" />",
                  "VisualsEditView": "<{$visualtype} Name=\"{$name}\" DataTag=\"{$entity}_{$name}\" Path=\"{$name}\"{'{$type}'=='Lookup' ? ' List=\"{$entity}_{$name}LookupList\"' : ''} />",
                  "{'{$type}'=='Lookup' ? 'LookupList' : ''}": "<LookupList Name=\"{$name}\" Label=\"[{$entity}_{$name}LookupListLabel]\" Description=\"[{$entity}_{$name}LookupListDescription]\">\n   <Value />\n</LookupList>"
                }
              }
            },
            {
              "id": "lookups",
              "name": "Lookup Lists",
              "applicableDocumentTypes": [
                "extension"
              ],
              "tokenReferences": [
                "lookupheader",
                "lookups"
              ],
              "type": "table",
              "injectionPaths": {
                "Lookups": "App/LookupLists",
                "Lookup Labels": "App/Cultures/Culture[@Code='en']/Labels"
              },
              "fragments": {
                "Lookups": {
                  "header": "<LookupList Name=\"{$entity}_{$listname}LookupList\" Label=\"{$entity}_{$listname}LookupListLabel\" Description=\"{$entity}_{$listname}LookupListDescription\">\n  <Value />",
                  "body": "  <Value Label=\"[{$entity}_{$listname}{$value}Label]\">{$value|friendly}</Value>",
                  "footer": "</LookupList>"
                },
                "Lookup Labels": {
                  "header": "<Label Key=\"{$entity}_{$listname}LookupListLabel\">{$listname|friendly}</Label>\n<Label Key=\"{$entity}_{$listname}LookupListDescription\">{$listname|friendly}</Label>",
                  "body": "<Label Key=\"{$entity}_{$listname}{$value}Label\">{'{$label}' != '' ? '{$label|friendly}' : '{$value|friendly}'}</Label>"
                }
              }
            },
            {
              "id": "supplements",
              "name": "Supplement Attributes",
              "applicableDocumentTypes": [
                "supplement"
              ],
              "tokenReferences": [
                "appname",
                "attributes"
              ],
              "injectionPaths": {
                "Attributes": "App.Supplements/Supplement/Supplement.EntityDefs/Update/Update.Attributes",
                "Labels": "App.Supplements/Supplement/Supplement.Cultures/Update[@Code='en']/Update.Labels",
                "LookupListLabels": "App.Supplements/Supplement/Supplement.Cultures/Update[@Code='en']/Update.Labels",
                "DataTags": "App.Supplements/Supplement/Supplement.DataTags",
                "LookupLists": "App.Supplements/Supplement/Supplement.LookupLists",
                "LookupList": "App.Supplements/Supplement/Supplement.LookupLists",
                "FieldDefs": "App.Supplements/Supplement/Supplement.HubDefs/Update/Update.LogDef/Update/Update.FieldDefs",
                "DataStore": "App.Supplements/Supplement/Supplement.DataStore/Update/Update.Tables/Update/Update.Columns"
              },
              "fragments": {
                "body": {
                  "Attributes": "<Add Name=\"{$name}\" Label=\"[{$entity}_{$name}Label]\" Description=\"[{$entity}_{$name}Description]\" DataType=\"{$type}\"{'{$type}'=='Lookup' ? ' LookupListName=\"{$appname|raw}.{$entity}_{$name}LookupList\"' : ''}{'{$type}'=='Entity' ? ' LinkedEntityDef=\"{$linkedEntityDef|raw}\"' : ''} IsConfigurable=\"true\" SupplementAction=\"Add\" />",
                  "Labels": "<Add Key=\"{$entity}_{$name}Label\" Value=\"{'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}\" />\n<Add Key=\"{$entity}_{$name}Description\" Value=\"{'{$descriptionlabel}'!='' ? '{$descriptionlabel|friendly}' : {'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}}\" />",
                  "{'{$type}'=='Lookup' ? 'LookupListLabels' : ''}": "<Add Key=\"{$entity}_{$name}LookupListLabel\" Value=\"{'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}\" />\n<Add Key=\"{$entity}_{$name}LookupListDescription\" Value=\"{'{$descriptionlabel}'!='' ? '{$descriptionlabel|friendly}' : {'{$label}'!='' ? '{$label|friendly}' : '{$name|friendly}'}}\" />",
                  "DataTags": "<Add Name=\"{$entity}_{$name}\" Label=\"[{$entity}_{$name}Label]\" CultureLabelKey=\"{$entity}_{$name}Label\" SupplementAction=\"Add\" />",
                  "Fields": "<Add Attribute=\"{$name}\" />",
                  "FieldDefs": "<Add Name=\"{$name}\" Path=\"{$name}\" DataTag=\"{$entity}_{$name}\" />",
                  "DataStore": "<Add AttributeName=\"{$name}\" />",
                  "LogFields": "<Add FieldDef=\"{$name}\" />",
                  "ImportDefs": "<Column AttributeName=\"{$name}\" Name=\"{$name|friendly}\" />",
                  "VisualsTextBlock": "<TextBlock Name=\"{$name}\" DataTag=\"{$entity}_{$name}\" Path=\"{$name}\" />",
                  "VisualsPreview": "<{$visualtype} Name=\"{$name}\" DataTag=\"{$entity}_{$name}\" Path=\"{$name}\" IsReadOnly=\"True\" />",
                  "VisualsEditView": "<{$visualtype} Name=\"{$name}\" DataTag=\"{$entity}_{$name}\" Path=\"{$name}\"{'{$type}'=='Lookup' ? ' List=\"{$entity}_{$name}LookupList\"' : ''} />",
                  "{'{$type}'=='Lookup' ? 'LookupList' : ''}": "<LookupList Name=\"{$name}\" Label=\"[{$entity}_{$name}LookupListLabel]\" Description=\"[{$entity}_{$name}LookupListDescription]\">\n   <Value />\n</LookupList>"
                }
              }
            },
            {
              "id": "test",
              "name": "Test Fragment",
              "tokenReferences": [
                "attributeheader",
                "attributes"
              ],
              "fragments": {
                "body": {
                  "TestElement": "<TestElement Name=\"{$name}\" Type=\"{$type}\" Label=\"{$label|friendly}\" Entity=\"{$entity}\" />",
                  "TestComment": "<!-- Test: {$name} of type {$type} for entity {$entity} -->"
                }
              }
            }
          ],
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier for this fragment definition"
              },
              "name": {
                "type": "string",
                "description": "Display name for this fragment definition"
              },
              "applicableDocumentTypes": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Optional list of document type IDs that can use this fragment. If omitted, the fragment can be used for any detected document type."
              },
              "tokenReferences": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Array of token name definition IDs to use for this fragment set"
              },
              "injectionPaths": {
                "type": "object",
                "description": "XPath-like paths for smart injection. Can be string (xpath only) or object with {path, displayAttribute}. The displayAttribute can be a string or array of strings (tries each in order until a value is found). Falls back to 'Name' if not specified, then to line number if no attributes have values. Examples: \"Attributes\": \"EntityDefs/EntityDef/Attributes\", \"Attributes\": {\"path\": \"EntityDefs/EntityDef/Attributes\", \"displayAttribute\": \"Name\"}, \"HubDefs\": {\"path\": \"App.HubDefs/HubDef\", \"displayAttribute\": [\"Label\", \"Name\"]}"
              },
              "fragments": {
                "type": "object",
                "description": "Fragment templates with token replacement"
              }
            },
            "required": [
              "id",
              "name",
              "tokenReferences",
              "fragments"
            ]
          },
          "description": "Fragment template definitions that reference token name definitions"
        },
        "kahua.menuOptions": {
          "type": "array",
          "default": [
            {
              "name": "Generate Extension Attributes",
              "fragments": [
                "attributes"
              ]
            },
            {
              "name": "Generate Supplement Attributes",
              "fragments": [
                "supplements"
              ]
            },
            {
              "name": "Generate Lookup Lists",
              "fragments": [
                "lookups"
              ]
            }
          ],
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "Display name for the menu option"
              },
              "fragments": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Array of fragment definition IDs to use for this menu option"
              }
            },
            "required": [
              "name",
              "fragments"
            ]
          },
          "description": "Context menu options that combine multiple fragment definitions"
        },
        "kahua.elementDisplayAttributes": {
          "type": "object",
          "description": "Configuration for how element names are displayed in generated XPath paths and quick pick labels.",
          "properties": {
            "defaultOrder": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "Name",
                "DisplayName",
                "Id"
              ],
              "description": "Default order of attributes to check for an element's display name if no element-specific override is found."
            },
            "exclusions": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "App"
              ],
              "description": "List of element tag names for which the name attribute should be excluded from display in the XPath path."
            },
            "overrides": {
              "type": "object",
              "additionalProperties": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "default": {
                "Table": [
                  "EntityDefName",
                  "Name"
                ],
                "ViewDef": [
                  "DisplayName",
                  "Name"
                ]
              },
              "description": "Element-specific overrides for the order of attributes to check for an element's display name. Key is the element tag name, value is an array of attribute names in preferred order."
            }
          }
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoffsam/kahua-attribute-generator.git"
  },
  "license": "MIT",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.10",
    "saxes": "^6.0.0"
  }
}
